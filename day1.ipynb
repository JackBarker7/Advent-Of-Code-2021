    "import numpy as np\n",
    "\n",
    "def depth_measurement(input_arr: list):\n",
    "    count = 0\n",
    "    for i, val in enumerate(input_arr):\n",
    "        if val>input_arr[i-1]:\n",
    "            count += 1\n",
    "    return count\n",
    "\n",
    "depth_measurement([199,200,208,210,200,207,240,269,260,263])\n",
